{
  "routes": [
    {
      "route": "/authenticate/index.html",
      "allowedRoles": ["authenticated"]
    },
       {
   "route": "/login",
       "rewrite": "/.auth/login/aad"
}
  ],
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "https://www.tonytech.in/login?post_login_redirect_uri=https://www.tonytech.in/index.html"
    },
    "404": {
      "statusCode": 404,
      "redirect": "404.html"
    }
  }
}
